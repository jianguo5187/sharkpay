    <delete id="deleteJndById" parameterType="Long">
        delete from jnd where id = #{id}
    </delete>

    <delete id="deleteJndByIds" parameterType="String">
        delete from jnd where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
