            <if test="sum3 != null "> and sum_3 = #{sum3}</if>
        </where>
    </select>
    
    <select id="selectJndById" parameterType="Long" resultMap="JndResult">
        <include refid="selectJndVo"/>
        where id = #{id}
    </select>

    <select id="selectLastRecord" resultMap="JndResult">
        <include refid="selectJndVo"/>
        order by id desc
        limit 1
    </select>

    <select id="selectJndListByParam" resultMap="JndResult">
        <include refid="selectJndVo"/>
        <if test="idDescFlg != null and idDescFlg != '' ">
            order by id desc
        </if>
        <if test="limitNumber != null">
            limit #{limitNumber}
        </if>
    </select>
        
    <insert id="insertJnd" parameterType="Jnd" useGeneratedKeys="true" keyProperty="id">
        insert into jnd(
            <if test="id != null">id,</if>
            <if test="time != null">time,</if>
            <if test="sum1 != null">sum_1,</if>
            <if test="sum2 != null">sum_2,</if>
            <if test="sum3 != null">sum_3,</if>
            <if test="createBy != null">create_by,</if>
        create_time
        )values(
            <if test="id != null">#{id},</if>
            <if test="time != null">#{time},</if>
            <if test="sum1 != null">#{sum1},</if>
            <if test="sum2 != null">#{sum2},</if>
            <if test="sum3 != null">#{sum3},</if>
            <if test="createBy != null">#{createBy},</if>
        sysdate()
        )
    </insert>

