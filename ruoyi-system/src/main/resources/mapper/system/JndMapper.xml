    <sql id="selectJndVo">
        select id, time, num1, num2, num3, num4, num5, num6, num7, num8, num9, num10, num11, num12, num13, num14, num15, num16, num17, num18, num19, num20, sum_1, sum_2, sum_3, create_by, create_time, update_by, update_time, remark from jnd
    </sql>

    <select id="selectJndList" parameterType="Jnd" resultMap="JndResult">
        <include refid="selectJndVo"/>
        <where>  
            <if test="time != null "> and time = #{time}</if>
            <if test="num1 != null "> and num1 = #{num1}</if>
            <if test="num2 != null "> and num2 = #{num2}</if>
            <if test="num3 != null "> and num3 = #{num3}</if>
